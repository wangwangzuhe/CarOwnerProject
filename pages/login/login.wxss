.login-container {
  position: absolute;
  left: 30rpx;
  top: 30rpx;
  bottom: 30rpx;
  right: 30rpx;
  padding: 60rpx;
  background-color: #212121;
  text-align: center;
  border-radius: 10rpx;
}
.login-container .vehicle-wrapper {
  margin-top: 170rpx;
}
.login-container .vehicle-wrapper image {
  width: 231rpx;
  height: 185rpx;
}
.login-container .rescue-slogan {
  margin-top: 30rpx;
  color: #fff;
}
.login-container .rescue-tip {
  margin-top: 30rpx;
}
.login-container .rescue-tip view {
  font-size: 20rpx;
  color: #fff;
}
.login-container .phone-wrapper {
  margin-top: 50rpx;
}
.login-container .phone-wrapper image {
  width: 54rpx;
  height: 75rpx;
  vertical-align: middle;
}
.login-container .login-tip1 {
  font-size: 24rpx;
  color: #f5b302;
  margin-top: 30rpx;
}
.login-container .login-box-wrapper .weui-label {
  width: 40rpx;
  height: 40rpx;
}
.login-container .login-box-wrapper .weui-vcode-btn {
  color: #000;
  font-size: 28rpx;
  background-color: #f5b302;
  border: 0;
}
.login-container .login-tip2 {
  font-size: 20rpx;
  color: #fff;
  margin-top: 10rpx;
}
.login-container .login-mode-wrapper {
  line-height: 70rpx;
  background-color: #101010;
  text-align: left;
  padding-left: 30rpx;
  margin-top: 300rpx;
}
.login-container .login-mode-wrapper text:first-child {
  font-size: 26rpx;
  color: #fff;
}
.login-container .login-mode-wrapper text:last-child {
  font-size: 20rpx;
  color: #666666;
}
.login-container .login-btn-wrapper {
  margin-top: 40rpx;
}
.login-container .login-btn-wrapper .login-btn {
  line-height: 90rpx;
  background-color: #f5b302;
  color: #000;
}
.login-container .wx-icon:before {
  content: '';
  display: inline-block;
  width: 35rpx;
  height: 29rpx;
  background-image: url('data:image/png;base64,');
  background-size: 35rpx 29rpx;
  margin-right: 20rpx;
}
