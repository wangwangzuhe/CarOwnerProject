.launchaid-title {
  height: 88rpx;
  display: flex;
  color: #fff;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #212121;
  margin-top: 20rpx;
  padding: 30rpx 50rpx;
  border-radius: 10rpx;
}
.faqi {
  flex: 5;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  color: #000;
  background-color: #f5b302;
}
.chakan {
  flex: 5;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  color: #fff;
  border: 1rpx solid #f5b302;
  margin-left: 10rpx;
}
.status-box {
  width: 354rpx;
  height: 615rpx;
  float: left;
  margin-left: 93rpx;
  position: relative;
  box-sizing: border-box;
}
.status-box .car-status {
  width: 354rpx;
  height: 90rpx;
}
.statusss {
  margin-top: 14rpx;
}
.statusss.one {
  margin-top: 70rpx;
}
.text {
  float: left;
  margin-top: 25rpx;
  width: 130rpx;
  white-space: nowrap;
  font-weight: bold;
  color: #8a8b90;
}
.text2 {
  float: left;
  margin-top: 25rpx;
  width: 120rpx;
  margin-left: 10rpx;
  white-space: nowrap;
  font-weight: bold;
  color: #8a8b90;
}
.sever-des {
  margin-top: 20rpx;
  background-color: #212121;
  padding: 60rpx 50rpx;
  border-radius: 10rpx;
}
.sever-des-item {
  flex: 1;
}
.back-btn {
  background-color: #f5b302;
  color: #000;
  text-align: center;
  line-height: 90rpx;
  margin-top: 60rpx;
}
.server-des-item2-c {
  font-size: 28rpx;
  line-height: 38rpx;
  margin-top: 8rpx;
}
.server-des-item2-c .service-content {
  margin-right: 30rpx;
  font-size: 28rpx;
}
.server-des-item2-c .service-content:before {
  content: '';
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  background-color: #fff;
  border-radius: 50%;
  margin-right: 10rpx;
  vertical-align: middle;
}
.nostart {
  width: 40px;
  height: 20px;
  vertical-align: middle;
  border-radius: 50%;
}
.txt {
  line-height: 100rpx;
  float: left;
  font-weight: bold;
}
.clear:after {
  clear: both;
  content: '';
  display: block;
}
.paianing,
.arraved,
.helping,
.company {
  margin-top: 0px;
}
.oper {
  line-height: 50px;
  background-color: #565f73;
}
.operstatus {
  margin-left: 20rpx;
  color: #fff;
  font-size: 12px;
  font-weight: bold;
  float: left;
}
.operdistace {
  float: right;
  padding: 0px 5px 0px 10px;
  font-weight: bold;
}
.operdistace .dista,
.operdistace .pretime {
  line-height: 25px;
  font-size: 12px;
  color: #fff;
}
.mapcontent {
  background: #21242e;
  box-sizing: border-box;
}
.distlabel {
  display: inline-block;
  width: 40px;
  text-align: center;
}
.header {
  position: relative;
  padding: 50rpx;
  background-color: #212121;
  border-radius: 10rpx;
}
.header .dashed-line {
  width: 85%;
  height: 0;
  border: 1rpx dashed #bfc1c5;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, 0);
}
.header .title {
  width: 100%;
  height: 85rpx;
  z-index: 10;
  bottom: 0;
  left: 0;
}
.header .title .title-name {
  width: 204rpx;
  height: 85rpx;
  margin: auto;
}
.header .rescue-style {
  text-align: center;
  display: flex;
  justify-content: center;
}
.header .rescue-style .defstyle .time text:first-child {
  display: inline-block;
  width: 10rpx;
}
.header .rescue-style .defstyle .time text:last-child {
  color: #8a8a8d;
  font-size: 26rpx;
}
.header .rescue-style .defstyle .time text.active {
  color: #f5b302;
}
.header .rescue-style .defstyle .circle {
  margin-top: 20rpx;
  font-size: 0;
  display: flex;
  align-items: center;
}
.header .rescue-style .defstyle .circle text {
  display: inline-block;
  height: 0;
  width: 10rpx;
  border: 1rpx dashed #bfc1c5;
}
.header .rescue-style .defstyle .circle image {
  width: 90rpx;
  height: 90rpx;
}
.header .rescue-style .defstyle .style-name {
  color: #8a8a8d;
  margin-top: 18.75rpx;
  text-align: right;
  padding-right: 18rpx;
  font-size: 26rpx;
}
.header .rescue-style .defstyle .style-name.active {
  color: #f5b302;
}
